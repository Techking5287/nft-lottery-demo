{
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf .blocklet dist",
    "bundle": "yarn clean && yarn build && blocklet bundle --zip --create-release",
    "deploy": "BASE_URL='/.blocklet/proxy' yarn bundle && blocklet deploy .blocklet/bundle",
    "release": "yarn bundle && blocklet publish .blocklet/release/blocklet.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "vite": "^2.3.7",
    "vite-preset-react": "^1.2.1"
  }
}
